        var map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -23.4721221, lng: -47.4449999},
          zoom: 18,
          mapTypeControlOptions: {
            mapTypeIds: ['roadmap', 'satellite', 'hybrid', 'terrain',
                    'styled_map']
